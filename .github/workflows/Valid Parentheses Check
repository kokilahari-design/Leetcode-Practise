20. Valid Parentheses
# valid if: Open brackets must be closed by the same type of brackets. Open brackets must be closed in the correct order.
# Every close bracket has a corresponding open bracket of the same type.


# Time Complexity = O(n)    Space Complexity = O(n)
class Solution(object):
    def isValid(self, s):
        d = { '(':')' , "{":'}' , "[":"]" }
        stack = []
        for i in s:
            if i in d.keys():
                stack.append(i) 
            else:
                if stack == []:                # If input = ")[]"  -> If beginning has closed parenthesis, then return False
                    return False     
                else:
                    pop_value = i
                    if d[stack[-1]] == pop_value :             # Fetch dictionary value of stack last element match with the pop_value 
                        stack.pop()
                    else:
                        return False
        if stack == []:
            return True
        else:
            return False
result = Solution()
print(result.isValid("()[]{}"))


---------------------------------------------------------------------------------------------------------------------------
# Optimized code:

class Solution(object):
    def isValid(self, s):
        stack = []
        d = {')': '(', '}': '{', ']': '['}
        
        for char in s:
            if char in d:  # closing bracket
                if not stack or stack[-1] != d[char]:
                    return False
                stack.pop()
            else:  # opening bracket
                stack.append(char)
                
        return not stack
